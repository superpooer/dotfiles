# vim: filetype=sh
alias h="cd ~/ && ls -A" \
d="cd ~/Documents && ls -A" \
D="cd ~/Downloads && ls -A" \
m="cd ~/Music && ls -A" \
pp="cd ~/Pictures && ls -A" \
vv="cd ~/Videos && ls -A" \
cf="cd ~/.config && ls -A" \
sc="cd ~/.local/bin && ls -A" \
mn="cd /mnt && ls -A" \
